    "keywords": ["laravel", "invite", "invitations"],
    "homepage": "https://github.com/rudolphp/larainvite",
    "license": "MIT",
    "authors": [
        {
            "name": "Rudolph Pereira",
            "email": "rudolph.pereira@gmail.com"
        }
    ],
    "require": {
